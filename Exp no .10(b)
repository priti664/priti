NAME :  VERMA. PRITI 
DIV. :  C  ROLL NO.12
UIN : 241P012

#include <stdio.h>

int main() {
    int arr[5], *ptr;
    
    printf("Enter 5 elements:\n");
    for (int i = 0; i < 5; i++) {
        scanf("%d", &arr[i]);
    }

    ptr = arr + 4; 

    printf("Array in reverse order:\n");
    for (int i = 0; i < 5; i++) {
        printf("%d ", *ptr);
        ptr--; 
    }

    printf("\n");

    return 0;
}


/* Output: 

Enter 5 elements:
3 7 9 8 6
Array in reverse order:
6 8 9 7 3

*/
