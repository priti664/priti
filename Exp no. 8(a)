#include <stdio.h>

int main() {
    char str[200];
    int length = 0;

    printf("Enter a string: ");
    scanf("%s",str);

    while (str[length] != '\0') {
        length++;
    }

    printf("The length of the string is: %d\n", length);

    return 0;
}

/* Output: 

Enter a string: Hello World
The length of the string is: 11

*/


